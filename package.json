{
  "name": "claude-react-agent-template",
  "version": "0.1.0",
  "description": "ReAct-style OpenAI agent template using the TypeScript SDK with web search & fetch tools",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/cli.ts",
    "dev": "tsx watch src/cli.ts",
    "check": "tsc --noEmit",
    "test": "node --import tsx --test \"test/**/*.test.ts\"",
    "cases": "tsx scripts/runDefaultCases.ts"
  },
  "dependencies": {
    "openai": "^4.78.1",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "undici": "^6.18.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5",
    "tsx": "^4.7.0"
  }
}
